{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Object with Boolean and Number",
  "type": "object",
  "properties": {
    "enabled": {
      "type": "boolean",
      "description": "Feature toggle"
    },
    "count": {
      "type": "number",
      "minimum": 0,
      "description": "A non-negative floating-point number"
    }
  },
  "required": ["enabled"]
}
